<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.g3pay" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>VitaTop</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#FF3D00" />
    <preference name="StatusBarStyle" value="default" />
    <preference name="NavigationBarBackgroundColor" value="#FFFFFF" />
    <preference name="NavigationBarLight" value="true" />
</widget>
